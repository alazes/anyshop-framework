{
  "name": "@anyshop/anyshop",
  "version": "0.0.0",
  "private": true,
  "contributors": [
    {
      "name": "Nelson Martell",
      "email": "nelson6e65@gmail.com",
      "url": "https://nelson6e65.github.io"
    },
    {
      "name": "RenÃ© Arias",
      "email": "rene_811@hotmail.com",
      "url": "https://renearias.com"
